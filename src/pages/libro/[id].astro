---




<LayoutVarios {breadcrumbs}>


    <!-- Shop Single Start -->
    <div id="rs-single-shop" class="rs-single-shop shop-rp orange-color pt-100 pb-100 md-pt-70 md-pb-70">
        <div class="container">
            <div class="row">
                <div class="col-lg-5 col-sm-12 sm-mb-30">
                    <div class="single-product-image">
                        <div class="img2p">
                            <img src={detalleLibroFilter?.imagen} alt="Single Product">
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <div class="single-price-info pl-30">
                        <h4 class="product-title">  {detalleLibroFilter?.titulo} </h4>
                            { 
                                detalleLibroFilter?.descripcion.map((desc) => 
                                    <p class="some-text">
                                        {desc}
                                    </p>
                                    <br/> 
                                    <br/>								
                                )
                            } 
                        
                        <div class="btn-part pull-left" data-wow-delay="400ms" data-wow-duration="2000ms">
                            <a class="readon2" href={detalleLibroFilter?.btnLink} target="_blank">{detalleLibroFilter?.btnNombre}</a>
                        </div>
                        
                    </div>
                </div> 
            </div>

            <div class="tab-area">
                <ul class="nav nav-tabs">
                    {
                        detalleLibroFilter?.tabs && detalleLibroFilter?.tabs.map((tab:any) => 
                            <li><a class:list={[ '', { "active": tab.mostrar }]} href={`#${tab.posicion}`} data-bs-toggle="tab">{tab.titulo}</a></li>
                        )
                    }
                </ul>

                <div class="tab-content">
                    {
                        detalleLibroFilter?.tabs && detalleLibroFilter?.tabs.map((tab:any) => 
                            <div class:list={[ 'tab-pane', { "active": tab.mostrar }]} id={tab.posicion}>
                                <div>
                                    {
                                        tab.obra && (
                                            <p class="dsc-p">
                                                <b>Derechos de autor:</b> {tab.obra.derechosAutor}<br />
                                                <b>Depósito Legal:</b> {tab.obra.depositoLegal}<br />	
                                                {
                                                    tab.obra.ISBN.map((isbn:any) =>
                                                        <b>ISBN:</b> <span> {isbn}</span><br />							
                                                    )
                                                }							
                                                <b>Tamaño:</b> {tab.obra.Tamaño}<br />
                                                <b>Número de páginas:</b> {tab.obra.numeroPag}<br />		
                                                {
                                                    tab.obra.evaluacionPares && (
                                                        <span>
                                                            <b>Evaluación de pares: </b> {tab.obra.evaluacionPares}<br />
                                                        </span>
                                                    )
                                                }			
                                                {
                                                    tab.obra.metodologiaEvaluacion && (
                                                        <span>
                                                            <b>Metodología de evaluación: </b> {tab.obra.metodologiaEvaluacion}<br /> <br />		
                                                        </span>
                                                    )
                                                }
                                                {
                                                    tab.obra.clasificacion && (
                                                        <span>
                                                            <b>Clasificación {tab.obra.clasificacion}</b><br />
                                                        </span>
                                                    )
                                                }
                                                
                                                {
                                                    tab.obra.parrafos && tab.obra.parrafos.map((parrafo:any) => 
                                                        <span>
                                                            {parrafo}
                                                        </span>
                                                        <br />
                                                    )
                                                }
                                                
                                                <br />

                                                {
                                                    tab.obra.palabrasClave && (
                                                        <span>
                                                            <b>Palabras clave: </b>  {tab.obra.palabrasClave}<br />
                                                        </span>
                                                    )
                                                }
                                                {
                                                    tab.obra.keyWords && (
                                                        <span>
                                                            <b>Key words: </b>  {tab.obra.keyWords}<br />
                                                        </span>
                                                    )
                                                }

                                                {
                                                    tab.obra.revisionCorreccion && (
                                                        <b>Revisión y Corrección: </b>
                                                        <br />
                                                        <span>
                                                            {
                                                                tab.obra.revisionCorreccion.map((rc:any) =>
                                                                    <span>
                                                                        {rc}
                                                                    </span>
                                                                    <br />
                                                                )
                                                            }
                                                        </span>
                                                    )
                                                }
                                                {
                                                    tab.obra.diseñoDiagramación && (
                                                        <b>Diseño y diagramación: </b>
                                                        <br />
                                                        <span>
                                                            {
                                                                tab.obra.diseñoDiagramación.map((dd:any) =>
                                                                    <span>
                                                                        {dd}
                                                                    </span>
                                                                    <br />
                                                                )
                                                            }
                                                        </span>
                                                    )
                                                }
                                                {
                                                    tab.obra.ilustradores && (
                                                        <b>Ilustradores: </b>
                                                        <br />
                                                        <span>
                                                            {
                                                                tab.obra.ilustradores.map((ilust:any) =>
                                                                    <span>
                                                                        {ilust}
                                                                    </span>
                                                                    <br />
                                                                )
                                                            }
                                                        </span>
                                                    )
                                                }
                                            </p>
                                        )
                                    }
                                </div>
                                {
                                    tab.autores && (
                                        <div class="container-fluid">
                                            {
                                                tab.autores.map((autor:any) =>
                                                    <div class="row">
                                                        <div class="col-md-6 col-sm-12">
                                                            {
                                                                autor.imagen && (
                                                                    <img src={autor.imagen} alt="" class="img-fluid img-thumbnail w-50">
                                                                )
                                                            }
                                                        </div>
                                                        
                                                        <div class="col-md-6 col-sm-12 pt-4">
                                                            <br>
                                                            
                                                                    <h6>{autor.nombre}</h6>
                                                                    {
                                                                        autor.enlaces && autor.enlaces.map((enl:any) =>
                                                                            <br/>
                                                                            <a href={enl.url}>{enl.enlace}</a>
                                                                            <br/>
                                                                        )
                                                                    }
                                                                    {
                                                                        autor.bibliografia && autor.bibliografia.map((bi:any) =>
                                                                            <p>{bi}</p>
                                                                        )
                                                                    }
                                                                    <br/>
                                                                    <br/>
                                                                
                                                        </div>
                                                    </div>
                                                )
                                            }
                                        </div>
                                    )
                                }
                            </div>
                        )
                    }
                </div>
            </div>
        </div>
    </div>



    <div class="contenedor">
        <CarrerasPorFacultad carreras={catalogoBibliotecaVirtual}>
            Catálogo de la Biblioteca virtual
        </CarrerasPorFacultad/>
    </div>


    
</LayoutVarios>